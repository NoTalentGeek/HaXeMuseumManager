<?xml encoding="utf-8" version="1.0" ?>
<scrollview height="200" width="100%" >
    <grid id="UIPopupAddObjectTag_Grid" width="100%" >

        <!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

        <text text="Name" />
        <textInput id="UIPopupAddObjectTag_InputName" text=" " width="100%" />
        
        <!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

        <text text="Feel" />
        <listSelector id="UIPopupAddObjectTag_SelectFeel" text=" " width="100%" >
            <array>POSITIVE, NEUTRAL, NEGATIVE</array>
        </listSelector>
        
        <!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

        <text text="General" />
        <listSelector id="UIPopupAddObjectTag_SelectGeneral" text=" " width="100%" >
            <array>Yes, No</array>
        </listSelector>
        
        <!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

        <text text="Tag Type" />
        <listSelector id="UIPopupAddObjectTag_SelectTagType" text=" " width="100%" >
            <array>
                ADJ,
                ADV,
                NOUN_ALIVE_ABSTRACT,
                NOUN_ALIVE_CONCRETE,
                NOUN_INANIMATE_HOLD_ABSTRACT,
                NOUN_INANIMATE_HOLD_CONCRETE,
                NOUN_INANIMATE_PLACE_ABSTRACT,
                NOUN_INANIMATE_PLACE_CONCRETE_NO_OWNER,
                NOUN_INANIMATE_PLACE_CONCRETE_OWNER,
                NOUN_INANIMATE_SEE_ABSTRACT,
                NOUN_INANIMATE_SEE_CONCRETE,
                NOUN_INANIMATE_TITLE,
                NOUN_INANIMATE_WEAR_ABSTRACT,
                NOUN_INANIMATE_WEAR_CONCRETE,
                VERB_INTRANSITIVE,
                VERB_TRANSITIVE
            </array>
        </listSelector>

        <!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

    </grid>
</scrollview>